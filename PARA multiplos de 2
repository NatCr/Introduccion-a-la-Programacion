#include "stdafx.h"
#include "conio.h"
#include <iostream>
 
using namespace std;
 
void main()
{
int N,I;
cout<<"Ingrese N:\n";
cin>>N;
for(I=1;I<=N;I=I*2)
cout<<I<<"\n";
getch();
} 

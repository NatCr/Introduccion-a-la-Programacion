#include "stdafx.h"
#include "conio.h"
#include <iostream>
 
using namespace std;
 
void main()
{
int I,N;
cout<<"Ingrese N:\n";
cin>>N;
I=2;
while(I<=N)
{cout<<I<<"\n";
I=I+2;}
getch();
}
  
